{
    "extends": [
        "tslint-eslint-rules",
        "tslint-consistent-codestyle",
        "tslint-react"
    ],
    "jsRules": {
        "no-empty": true
    },
    "rules": {
        "prefer-const": true,
        "object-shorthand-properties-first": true,
        "space-before-function-paren": [
            true,
            {
                "anonymous": "always",
                "named": "never"
            }
        ],
        "no-this-assignment": true,
        "no-duplicate-imports": true,
        "no-boolean-literal-compare": false,
        "no-parameter-reassignment": true,
        "space-in-parens": [
            true,
            "never"
        ],
        "strict-boolean-expressions": [
            true,
            "allow-null-union",
            "allow-undefined-union",
            "allow-string",
            "allow-number",
            "allow-mix"
        ],
        "no-else-after-return": true,
        "ter-prefer-arrow-callback": [
            true
        ],
        "brace-style": [
            true,
            "1tbs",
            {
                "allowSingleLine": true
            }
        ],
        "array-bracket-spacing": [
            true,
            "never"
        ],
        "ter-indent": [
            true,
            4,
            {
                "SwitchCase": 1
            }
        ],
        "object-curly-spacing": [
            true,
            "always"
        ],
        "ter-arrow-parens": [2, "as-needed", { "requireForBlockBody": true }],
        "no-null-keyword": false,
        "only-arrow-functions": [
            false
        ],
        "component-selector": false,
        "no-access-missing-member": false,
        "array-type": [
            false
        ],
        "no-parameter-properties": false,
        "max-file-line-count": [
            false
        ],
        "cyclomatic-complexity": [
            false
        ],
        "prefer-for-of": false,
        "max-classes-per-file": [
            false
        ],
        "no-unused-css": false,
        "align": [
            true,
            "parameters",
            "statements"
        ],
        "ban": false,
        "class-name": true,
        "comment-format": [
            true,
            "check-space"
        ],
        "completed-docs": [
            false
        ],
        "curly": true,
        "eofline": true,
        "forin": true,
        "indent": [
            true,
            "spaces",
            4
        ],
        "interface-name": false,
        "jsdoc-format": true,
        "label-position": true,
        "linebreak-style": [
            true,
            "LF"
        ],
        "max-line-length": [
            true,
            140
        ],
        "member-access": [
            true,
            "check-accessor",
            "check-constructor"
        ],
        "member-ordering": [
            true,
            "public-before-private",
            "static-before-instance",
            "variables-before-functions"
        ],
        "new-parens": true,
        "no-angle-bracket-type-assertion": true,
        "no-any": false,
        "no-arg": true,
        "no-bitwise": false,
        "no-conditional-assignment": true,
        "no-consecutive-blank-lines": [
            true
        ],
        "no-console": [
            true
        ],
        "no-construct": true,
        "no-debugger": true,
        "no-default-export": false,
        "no-duplicate-variable": true,
        "no-empty": true,
        "no-eval": true,
        "no-inferrable-types": [
            false
        ],
        "no-internal-module": true,
        "no-invalid-this": true,
        "no-reference": true,
        "no-mergeable-namespace": true,
        "no-namespace": true,
        "no-require-imports": false,
        "no-shadowed-variable": true,
        "no-string-literal": true,
        "no-switch-case-fall-through": true,
        "no-trailing-whitespace": true,
        "no-unused-expression": true,
        "no-unused": [true,"ignore-imports"],
        "no-var-keyword": true,
        "no-var-requires": false,
        "object-literal-sort-keys": false,
        "one-line": [
            true,
            "check-catch",
            "check-else",
            "check-open-brace",
            "check-whitespace"
        ],
        "one-variable-per-declaration": [
            true,
            "ignore-for-loop"
        ],
        "ordered-imports": [
            false
        ],
        "quotemark": [
            true,
            "single",
            "jsx-double"
        ],
        "radix": true,
        "semicolon": [
            true,
            "always"
        ],
        "switch-default": true,
        "trailing-comma": [
            true,
            {
                "multiline": "never",
                "singleline": "never"
            }
        ],
        "triple-equals": [
            true,
            "allow-null-check"
        ],
        "typedef": [
            true,
            "call-signature",
            "parameter",
            "property-declaration"
        ],
        "typedef-whitespace": [
            true,
            {
                "call-signature": "nospace",
                "index-signature": "nospace",
                "parameter": "nospace",
                "property-declaration": "nospace",
                "variable-declaration": "nospace"
            }
        ],
        "use-isnan": true,
        "variable-name": [
            true,
            "ban-keywords",
            "check-format",
            "allow-leading-underscore",
            "allow-pascal-case"
        ],
        "whitespace": [
            true,
            "check-branch",
            "check-decl",
            "check-operator",
            "check-module",
            "check-separator",
            "check-type",
            "check-typecast",
            "check-preblock"
        ],
        "pipe-impure": false,
        "adjacent-overload-signatures": true,
        "arrow-parens":  [2, "as-needed", { "requireForBlockBody": true }],
        "file-header": [
            false
        ],
        "no-for-in-array": false,
        "no-unsafe-finally": true,
        "object-literal-key-quotes": [
            true,
            "as-needed"
        ],
        "object-literal-shorthand": true,
        "restrict-plus-operands": false,
        "jsx-no-multiline-js": false,
        "jsx-wrap-multiline": true
    }
}
